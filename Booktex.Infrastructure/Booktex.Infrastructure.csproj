<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NSwag.MSBuild" Version="14.2.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Booktex.Application\Booktex.Application.csproj" />
  </ItemGroup>
  
  
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="$(NSwagExe_Net90) run Clients/Docktex/DocktexAPIClient.nswag" Condition="1 == 2"></Exec>
    <Exec Command="$(NSwagExe_Net90) run Clients/GitHub/GitHubAPIClient.nswag" Condition="1 == 2"></Exec>

  </Target>
    

</Project>
